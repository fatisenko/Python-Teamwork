    "def cup_swapping(liste):\n",
    "  liste2 = [\"A\", \"B\", \"C\"]\n",
    "  b = 1\n",
    "  for i in liste:\n",
    "    if i[0] == liste2[b]:\n",
    "      b = liste2.index(i[1])\n",
    "    elif i[1] == liste2[b]:\n",
    "     b = liste2.index(i[0])   \n",
    "  return liste2[b]\n",
    "cup_swapping([\"BA\", \"AC\", \"CA\", \"BC\"])  "
   ]
